{
  "name": "cuddly-waffle-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/concurrently --kill-others \"npm run serve:server\" \"npm run serve:client\"",
    "serve:server": "ts-node --project ./server/tsconfig.json ./server/api.ts",
    "serve:client": "vue-cli-service serve",
    "build": "vue-cli-service build && tsc --project  ./server/ --outDir dist",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "now-build": "vue-cli-service build && tsc --project  ./server/ --outDir dist",
    "heroku-postbuild": "./node_modules/.bin/concurrently --kill-others \"npm run serve:server\" \"npm run serve:client\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.3",
    "color-convert": "^2.0.0",
    "connect-timeout": "^1.9.0",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "normalize.css": "^8.0.1",
    "puppeteer": "^1.14.0",
    "register-service-worker": "^1.6.2",
    "request-promise": "^4.2.4",
    "ts-node": "^8.0.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-simple-svg": "^1.3.2",
    "vuex": "^3.0.1",
    "concurrently": "^4.1.0",

    "@vue/cli-service": "^3.6.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.0",
    "@types/cheerio": "^0.22.11",
    "@types/color-convert": "^1.9.0",
    "@types/connect-timeout": "0.0.34",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.1.4",
    "@types/puppeteer": "^1.12.3",
    "@types/request-promise": "^4.1.42",
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-pwa": "^3.6.0",
    "@vue/cli-plugin-typescript": "^3.6.0",
    "@vue/cli-plugin-unit-jest": "^3.6.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "fibers": "^3.1.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "style-resources-loader": "^1.2.1",
    "tailwindcss": "^0.7.4",
    "ts-jest": "^23.0.0",
    "typescript": "^3.4.5",
    "vue-svg-inline-loader": "^1.2.15",
    "vue-template-compiler": "^2.5.21"
  }
}
